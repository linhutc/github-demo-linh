                                    data-image="https://f.howkteam.vn/Upload/cke/images/1_LOGO%20SHOW%20WEB/2_CPlusPlus/1_CPlusPlus%20C%C6%A1%20b%E1%BA%A3n/CPP%20Basic%20howkteam%2042.jpg"
