		$i('.block-banner').cycle({ 
		   	fx: 'fade',
		    speed: 'fast', 
		    timeout: 10000, 
		   	next: '#next', 
	    	prev: '#prev',
	    	// before: function () {
      //       	$i('.caption').html($i(this).attr('alt'));
      //       	$i('.caption').html($i(this).find('img').attr('alt'));
      //       }
		});
    }

	});
</script>
